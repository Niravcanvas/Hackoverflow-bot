{
  "name": "hackoverflow-discord-bot",
  "version": "2.0.0",
  "description": "Discord bot for HackOverflow 4.0 hackathon with Groq AI integration and persistent queue",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "test:env": "ts-node src/test-env.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "hackathon",
    "typescript",
    "groq",
    "ai"
  ],
  "author": "HackOverflow Team",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "groq-sdk": "^0.8.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "overrides": {
    "undici": "^6.23.0"
  }
}